
<script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
<script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>


<div style = "padding:0.5rem; display:flex; justify-content:space-between">
    <a href = {{this.url}} target = "_blank">
        <span class="icon" style = "display:flex">    
            <ion-icon name="document-closed"></ion-icon>
            <div>{{this.url}}</div>
        </span>
    </a>
    
    {{#if (req.session.user_id === project.user_id)}}
        <div style = "display:flex">
            <span id = "download" class="icon" style = "display:flex">
                <ion-icon name="cloud-down-outline"></ion-icon>
            </span>
            <span id = "delete" class="icon" style = "display:flex">
                <ion-icon name="trash-outline"></ion-icon>
            </span>
        </div>
    {{/if}}
</div>